        <script type="text/javascript">

            $csnQ( function(){

                //
                // Wait to initialize events until the postMessage "handshake" is complete
                // -- iframe sends csn.INIT to parent, parent sends csn.INIT back
                //
                $csnQ( document ).on( "csn_iframe:message_ready", function() {

                    EventsModule.init();
                });

                window.addEventListener( "message", receiveMessage, false );
                function receiveMessage(message)
                {
                    //
                    // Ignore messages that aren't strings
                    //
                    if( typeof message.data !== "string" || message.data.indexOf( "csn." ) < 0  ) return;

                    //
                    // Ignore non csn. messages -- we really only care about listening for INIT response from the parent
                    //
                    if( message.data.indexOf( "csn.INIT" ) >= 0 )
                    {
                        //
                        // Parent communication channel established, trigger other dependent actions
                        //
                        $csnQ( document ).trigger( "csn_iframe:message_ready" );
                    }
                }

                //
                // Send INIT message to parent
                //
                window.parent.postMessage( "csn.INIT", "*" );

            });



        </script>

